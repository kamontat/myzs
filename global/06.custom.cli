#!/usr/bin/env bash
# shellcheck disable=SC1090,SC2015

# maintain: Kamontat Chantrachirathumrong
# version:  1.0.0
# since:    21/04/2018

alias color-test='bash <(curl -sL https://raw.githubusercontent.com/kamontat/bash-color/master/color_test.sh)' &&
	_myzs_print_debug_tofile "custom" "cli" "color-test => (https://github.com/kamontat/bash-color)"

alias split='bash <(curl -sL https://gist.githubusercontent.com/kamontat/97ea3378c5d323b9b430aeda8b5f80df/raw/dbda55f2cadc0481b3a27f4ffc13602c6b7ffe8f/sep.sh)' &&
	_myzs_print_debug_tofile "custom" "cli" "split => (https://gist.github.com/kamontat/97ea3378c5d323b9b430aeda8b5f80df/)"

# my dot template (https://github.com/kamontat/dot-github-mini)
_myzs_if_file_exist "$HOME/.github/dotgithub" &&
	alias dotgithub='$HOME/.github/dotgithub' &&
	_myzs_print_debug_tofile "custom" "cli" "dotgithub => $HOME/.github/dotgithub (https://github.com/kamontat/dot-github-mini)" ||
	_myzs_print_warning_tofile "custom" "cli" "dotgithub not exist!"

return 0
